# Makefile da inserire nella root dei sorgenti del progetto.
JFLAGS = -g
JC = javac
P = puzzle
S = solver
IO = fileinputoutput
E = myexception
L = logger

compile:
	$(JC) $(E)/*.java
	$(JC) $(P)/*.java
	$(JC) $(S)/*.java
	$(JC) $(IO)/*.java
	$(JC) $(L)/*.java
	$(JC) *.java
	@echo "Compilazione completata"

